application/vnd.oasis.opendocument.presentation; ~/.local/bin/mutt-launch %s libreoffice
application/vnd.oasis.opendocument.spreadsheet; ~/.local/bin/mutt-launch %s libreoffice
application/vnd.oasis.opendocument.text; ~/.local/bin/mutt-launch %s libreoffice
application/vnd.openxmlformats-officedocument.wordprocessingml.document; ~/.local/bin/mutt-launch %s libreoffice
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; ~/.local/bin/mutt-launch %s libreoffice
application/x-sh; ~/.local/bin/mutt-launch %s gvim -R -n

